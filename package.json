{
  "name": "prosemirror-suggestcat-plugin",
  "version": "1.1.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c --bundleConfigAsCjs",
    "rebuild": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c -w --bundleConfigAsCjs",
    "format": "eslint src --ext .ts --fix",
    "prepublishOnly": "pnpm run build && pnpm run lint",
    "version": "pnpm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "lint": "tsc --noEmit && eslint src --ext .ts",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:blockRunner": "playwright test --project=blockRunner",
    "test:e2e:completeV2": "playwright test --project=completeV2",
    "test:e2e:grammarV2": "playwright test --project=grammarSuggestV2",
    "e2e:server:blockRunner": "vite --config e2e/blockRunner/vite.config.ts",
    "e2e:server:completeV2": "vite --config e2e/completeV2/vite.config.ts",
    "e2e:server:grammarV2": "vite --config e2e/grammarSuggestV2/vite.config.ts",
    "upgrade-interactive": "npm-check --update",
    "publish:np": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emergence-engineering/prosemirror-suggestcat-plugin.git"
  },
  "files": [
    "dist/**/*"
  ],
  "author": "Emergence Engineering",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emergence-engineering/prosemirror-suggestcat-plugin/issues"
  },
  "homepage": "https://github.com/emergence-engineering/prosemirror-suggestcat-plugin#readme",
  "dependencies": {
    "@emergence-engineering/fast-diff-merge": "^0.0.5",
    "@emergence-engineering/prosemirror-text-map": "^0.1.4",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^5.0.0",
    "fast-diff": "^1.3.0",
    "lodash.debounce": "^4.0.8",
    "prosemirror-model": "^1.19.3",
    "prosemirror-state": "^1.4.3",
    "prosemirror-test-builder": "^1.1.1",
    "prosemirror-transform": "^1.7.3",
    "prosemirror-view": "^1.31.4",
    "rimraf": "^6.0.1",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/jest": "^29.5.1",
    "@types/lodash.debounce": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "jest": "^29.5.0",
    "np": "^7.6.2",
    "npm-check": "^6.0.1",
    "prettier": "^3.0.0",
    "prosemirror-example-setup": "^1.2.2",
    "prosemirror-schema-basic": "^1.2.2",
    "rollup": "^3.7.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-minification": "^0.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0"
  }
}
